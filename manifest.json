{
    "name": "@walletX",
    "description": "Transfer funds to any wallet",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": "favicon.png"
    },
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png"
    },

    "permissions": ["tabs", "bookmarks", "unlimitedStorage", "nativeMessaging"]
}